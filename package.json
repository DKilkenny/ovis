{
  "name": "OVis",
  "version": "1.0.0",
  "description": "A visualization tool for OpenMDAO",
  "main": "main.js",
  "repository": "https://github.com/OpenMDAO/zune",
  "keywords": [
    "OpenMDAO",
    "Visualization"
  ],
  "author": "NASA Glenn Research Center",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "electron": "~2.0.2",
    "electron-builder": "^20.15.1",
    "electron-packager": "^12.1.0",
    "mocha": "^5.2.0",
    "spectron": "^3.8.0"
  },
  "dependencies": {
    "electron-is": "^2.4.1",
    "electron-log": "^2.2.14",
    "is-electron": "^2.1.0",
    "pickle": "^0.2.0",
    "promise": "^8.0.1",
    "request": "^2.87.0",
    "sqlite3": "^4.0.0",
    "wine": "^0.9.8"
  },
  "build": {
    "appId": "nasa.grc.ovis",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "extraResources": [
      "data_server",
      "components",
      "main.py"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "dev": "NODE_ENV='development' npm run start",
    "build": "./build_electron.sh",
    "test:e2e": "./node_modules/mocha/bin/mocha frontend-tests/frontend_tests.js"
  },
  "postinstall": "electron-builder install-app-deps"
}
